{
    "name": "hedii/extractors",
    "type": "library",
    "description": "Extractor is a package that find targeted types of resources in html dom.",
    "keywords": [
        "hedii",
        "extractors"
    ],
    "homepage": "https://github.com/hedii/extractors",
    "license": "MIT",
    "authors": [
        {
            "name": "Hedi Chaibi",
            "email": "contact@hedichaibi.com",
            "homepage": "https://hedichaibi.com"
        }
    ],
    "require": {
        "php" : ">=7.1",
        "symfony/css-selector": "^4.0",
        "symfony/dom-crawler": "^4.0"
    },
    "require-dev": {
        "laravel/lumen-framework": "5.5.*",
        "phpunit/phpunit": "~6.0"
    },
    "autoload": {
        "psr-4": {
            "Hedii\\Extractors\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Hedii\\Extractors\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    }
}
